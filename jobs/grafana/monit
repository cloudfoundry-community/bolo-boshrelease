check process grafana
  with pidfile /var/vcap/sys/run/grafana/grafana.pid
  start program "/var/vcap/jobs/grafana/bin/monit_debugger grafana_ctl '/var/vcap/jobs/grafana/bin/ctl start'"
  stop program "/var/vcap/jobs/grafana/bin/monit_debugger grafana_ctl '/var/vcap/jobs/grafana/bin/ctl stop'"
  group vcap
